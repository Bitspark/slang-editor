{
  "name": "slang-studio",
  "version": "1.0.0",
  "description": "Web application for Slang, a visual programming language.",
  "scripts": {
    "build": "webpack --config webpack/webpack.config.prod.js --colors",
    "start": "webpack-dev-server --open --config webpack/webpack.config.dev.js",
    "start-embedded": "npm start -- --env.slang-mode embedded",
    "build-embedded": "webpack --config webpack/webpack.config.prod.js --colors --env.slang-mode embedded",
    "lint": "tslint -p tsconfig.json -c tslint.json src/**/*.ts",
    "test": "jasmine-ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wbkd/webpack-starter.git"
  },
  "keywords": [
    "slang",
    "ide",
    "frontend",
    "visual programming"
  ],
  "author": "Bitspark GmbH",
  "license": "Apache License 2.0",
  "bugs": {
    "url": "https://github.com/Bitspark/slang-studio/issues"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@types/plotly.js": "^1.41.0",
    "backbone": "^1.3.3",
    "dagre": "^0.8.2",
    "graphlib": "^2.1.5",
    "jointjs": "github:Bitspark/joint",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "mithril": "^1.1.6",
    "plotly.js": "^1.42.5",
    "rxjs": "^6.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@fortawesome/fontawesome-free": "^5.5.0",
    "@types/backbone": "^1.3.43",
    "@types/jasmine": "^3.3.8",
    "@types/jquery": "^3.3.22",
    "@types/lodash": "^4.14.117",
    "@types/mithril": "^1.1.13",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.8.0",
    "eslint-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^4.0.0-beta.2",
    "jasmine": "^3.3.1",
    "jasmine-ts": "^0.3.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-consistent-codestyle": "^1.15.0",
    "typescript": "^3.2.4",
    "webpack": "^4.25.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  }
}
